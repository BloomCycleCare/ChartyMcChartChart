<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/main">

    <fragment
        android:id="@+id/main"
        android:name="com.bloomcyclecare.cmcc.practitioner.ui.main.MainFragment"
        android:label="Main"
        tools:layout="@layout/fragment_main">

        <action
            android:id="@+id/create_applink"
            app:destination="@+id/applink"/>
    </fragment>

    <fragment
        android:id="@+id/applink"
        android:name="com.bloomcyclecare.cmcc.practitioner.ui.applink.AppLinkFragment"
        android:label="App Link"
        tools:layout="@layout/fragment_applink"/>

</navigation>
